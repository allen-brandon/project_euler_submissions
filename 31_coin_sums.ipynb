    "from functools import cache\n",
    "\n",
    "\n",
    "coins = [1, 2, 5, 10, 20, 50, 100, 200]\n",
    "@cache\n",
    "def dfs(rem, idx):\n",
    "    if not rem: return 1\n",
    "    if idx == 8: return 0\n",
    "    if rem < 0: return 0\n",
    "    return dfs(rem-coins[idx], idx)+dfs(rem, idx+1)\n",
    "print(dfs(200, 0))"
   ]
